@model reviewsite.Model.Category


@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <link rel="stylesheet" type="text/css" href="~/CSS/StyleSheet.css">
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>
    <header>
        <h1>@Model.Name </h1>
        <section id="offWhite">
            <span><strong>@Model.Name</strong></span>
        </section>
    </header>
    <main>
        <h1>Product List</h1>
        @foreach (var product in Model.Products)
        {
            <p> @product.Name</p>
            <hr />
        }
            <a href="~/Category/Index/">Return</a>
    </main>
</body>
</html>
